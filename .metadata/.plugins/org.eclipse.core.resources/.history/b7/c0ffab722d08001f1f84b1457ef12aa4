package quiz;

import java.util.ArrayList;
import java.util.List;

abstract class Geometryy{
	List<Integer> xArr = new ArrayList<>();
	List<Integer> yArr = new ArrayList<>();
	abstract double getArea();
	abstract double getLength();
}

class Point extends Geometryy{
	
	public Point(int x, int y) {
		xArr.add(x);
		yArr.add(y);
	}

	@Override
	double getArea() {
		return 0;
	}

	@Override
	double getLength() {
		return 0;
	}
	
}

class Line extends Geometryy{

	@Override
	double getArea() {
		return 0;
	}

	@Override
	double getLength() {
		return 0;
	}
	
}

class PolyLine extends Geometryy{

	@Override
	double getArea() {
		return 0;
	}

	@Override
	double getLength() {
		return 0;
	}
	
}

public class GeometryMethods {
	

}
