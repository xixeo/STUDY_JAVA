package quiz;

abstract class Geometry1 {
	int[] xArr;
	int[] yArr;
	
	public abstract double getArea();
	public abstract double getLength();
}

class Point extends Geometry1 {
	int x;
	int y;
	
	public Point(int x, int y) {
		this.x = x;
		this.y = y;		
	}
	
	public void getArea();
}

public class Geometry{
	public static void main(String[] args) {
	
	}
}